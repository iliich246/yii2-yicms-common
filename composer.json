{
    "name": "iliich246/yii2-yicms-common",
    "description": "Base of the yicms",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "iliich246",
            "email": "iliich246@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-imagine": "*",
        "yiidoc/yii2-redactor": "*",
        "npm-asset/bootbox": "~4.4.0",
        "npm-asset/lodash": "~4.17.4",
        "npm-asset/cropper": "~3.1.4",
        "npm-asset/photoswipe": "~4.1.2"
